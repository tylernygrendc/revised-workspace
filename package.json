{
  "name": "workspace",
  "version": "0.10",
  "exports": {
    "./_*.js": "./src/scripts/_*.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "compile": "node pug.config.js && sass --no-source-map src/styles:dist/styles && rollup --config rollup.config.js",
    "publish": "node pug.config.production.js && npm run sass && rollup --config rollup.config.production.js",
    "compress": "npm run publish && zip ./dist/* && mkdir compressed && mv ./dist/*.zip ./dist.zip",
    "pug": "node pug.config.js",
    "rollup": "rollup --config rollup.config.js",
    "sass": "sass --no-source-map src/styles:dist/styles src/styles/mdc:src/styles/mdc",
    "serve": "http-server ./dist -c-1 -o"
  },
  "type": "module",
  "author": "tylernygren@gmail.com",
  "license": "GPL-3.0-only",
  "description": "",
  "dependencies": {
    "@material/web": "^2.2.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/pug": "^2.0.10",
    "@types/ttf2woff2": "^2.0.2",
    "chalk": "^5.3.0",
    "pug": "^3.0.3",
    "rollup": "^4.24.0",
    "sass": "^1.80.3",
    "ttf2woff2": "^5.0.0"
  }
}
